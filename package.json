    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.3",
    "mongoose": "^6.3.4",
    "nodemon": "^2.0.21",
    "validator": "^13.7.0"
  }
